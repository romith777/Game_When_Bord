{
    "manifest_version": 3,
    "name": "YOU CANT TOUCH ME",
    "version": "0.0.1",
    "description": "DARE TO TOUCH!",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "host_permissions": [
      "https://*/*"
    ]
}